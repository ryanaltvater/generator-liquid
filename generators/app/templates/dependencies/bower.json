{
  "name": "<%= projectNameLower %>",
  "version": "0.0.0",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "jquery": "3.2.1",
    "modernizr": "^3.5.0",
    "normalize.css": "^7.0.0"
  },
  "install": {
    "path": {
      "css": "src/assets/css/scss/bower",
      "scss": "src/assets/css/scss/bower",
      "js": "src/assets/js"
    },
    "sources": {
      "jquery": "bower_components/jquery/jquery.min.js",
      "modernizr": "bower_components/modernizr/modernizr.js",
      "normalize.css": {
        "mapping": [
          {
            "bower_components/normalize.css/normalize.css": "_normalize.scss"
          }
        ]
      }
    }
  },
  "license": "MIT"
}